_G.Out = true

function OutInvOrb_Shadow()
    if not game.Players.LocalPlayer.Backpack:FindFirstChild("Orb Atomic") and not workspace.Mob:FindFirstChild("Cid") and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid.Health >= 1 then
        local args = {
            [1] = "Orb Atomic"
        }
        
        game:GetService("ReplicatedStorage").Remotes.Inventory:FireServer(unpack(args))
        wait(0.5)
    end
end
task.spawn(function()
    while wait(1) do
        pcall(function()
            if _G.Out then
                OutInvOrb_Shadow()
            end
        end)
        wait(5)
    end
end)
